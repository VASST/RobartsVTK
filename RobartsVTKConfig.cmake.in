#-----------------------------------------------------------------------------
#
# RobartsVTKConfig.cmake - CMake configuration file for external projects.
#
# This file is configured by RobartsVTK and used by the UseRobartsVTK.cmake 
# module to load RobartsVTK's settings for an external project.

# The source dir
SET(RobartsVTK_SOURCE_DIR "@RobartsVTK_SOURCE_DIR_CONFIG@")

# The kits.
SET(RobartsVTK_KITS "@RobartsVTK_KITS_CONFIG@")

# The libraries.
SET(RobartsVTK_LIBRARIES "@RobartsVTK_LIBRARIES_CONFIG@")

# The library directories.
SET(RobartsVTK_LIBRARY_DIRS "@RobartsVTK_LIBRARY_DIRS_CONFIG@")

# The include directories.
SET(RobartsVTK_INCLUDE_DIRS "@RobartsVTK_INCLUDE_DIRS_CONFIG@")

# The "use" file.
SET(RobartsVTK_USE_FILE "@RobartsVTK_USE_FILE_CONFIG@")

# The VTK options.
SET(RobartsVTK_VTK_DIR "@Robarts_VTK_DIR_CONFIG@")

# The ITK options
SET(RobartsVTK_USE_ITK "@RobartsVTK_USE_ITK_CONFIG@")
SET(RobartsVTK_ITK_DIR "@RobartsVTK_ITK_DIR_CONFIG@")

# The libxml2 options
#SET(RobartsVTK_USE_LIBXML2 "@RobartsVTK_USE_LIBXML2_CONFIG@")
#SET(LIBXML2_FOUND "@LIBXML2_FOUND_CONFIG@")

# AtamaVTK_USE_SYSTEM_DCMTK
#SET(RobartsVTK_USE_SYSTEM_DCMTK "@RobartsVTK_USE_SYSTEM_DCMTK_CONFIG@")

SET (RobartsVTK_USE_VIDEOSOURCE2   "@RobartsVTK_USE_VIDEOSOURCE2@")
SET (RobartsVTK_USE_DIRECTSHOW     "@RobartsVTK_USE_DIRECTSHOW@")
SET (RobartsVTK_USE_EPIPHAN     "@RobartsVTK_USE_EPIPHAN@")
SET (RobartsVTK_USE_OPENCV_VIDEOSOURCE   "@RobartsVTK_USE_OPENCV_VIDEOSOURCE@")
SET (RobartsVTK_USE_FREEDOM6S      "@RobartsVTK_USE_FREEDOM6S@")

SET (RobartsVTK_USE_HEARTSIGNALBOX    "@RobartsVTK_USE_HEARTSIGNALBOX@")
SET (RobartsVTK_USE_DATABOX      "@RobartsVTK_USE_DATABOX@")
SET (RobartsVTK_USE_USBECGBOX    "@RobartsVTK_USE_USBECGBOX@")
SET (RobartsVTK_USE_LEGO     "@RobartsVTK_USE_LEGO@")


SET (RobartsVTK_USE_ROBARTSHAPTICS   "@RobartsVTK_USE_ROBARTSHAPTICS@")
SET (RobartsVTK_USE_ROBARTSLEGACY   "@RobartsVTK_USE_ROBARTSLEGACY@")
SET (RobartsVTK_USE_ROBARTSECG     "@RobartsVTK_USE_ROBARTSECG@")
SET (RobartsVTK_USE_ROBARTSVIDEO  "@RobartsVTK_USE_ROBARTSVIDEO@")
SET (RobartsVTK_USE_SONIX       "@RobartsVTK_USE_SONIX@")
SET (RobartsVTK_USE_MWIERZ     "@RobartsVTK_USE_MWIERZ@")
SET (RobartsVTK_USE_CUDA     "@RobartsVTK_USE_CUDA@")
SET (RobartsVTK_USE_ANIS     "@RobartsVTK_USE_ANIS@")
SET (RobartsVTK_USE_ROBARTSULTRASOUND  "@RobartsVTK_USE_ROBARTSULTRASOUND@")

# The library dependencies file.
IF(NOT RobartsVTK_NO_LIBRARY_DEPENDS)
  INCLUDE("@RobartsVTK_LIBRARY_DEPENDS_FILE@")
ENDIF(NOT RobartsVTK_NO_LIBRARY_DEPENDS)
